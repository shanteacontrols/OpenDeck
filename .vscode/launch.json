{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tests/build/test.out",
            "args": [],
            "stopAtEntry": false,
            "environment": [],
            "externalConsole": false,
            "linux": {
                "cwd": "${workspaceFolder}",
                "MIMode": "gdb"
            },
            "osx": {
                "cwd": "${workspaceFolder}",
                "MIMode": "lldb"
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "[Tests] Build"
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "bmp",
            "cwd": "${workspaceRoot}",
            "executable": "./src/build/app/discovery/debug/discovery.elf",
            "name": "[Discovery/FW]",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/misc/STM32F40x.svd",
            "device": "STM32F407",
            "BMPGDBSerialPort": "/dev/ttyBmpGdb",
            "targetId": 1,
            "preLaunchTask": "[FW/STM32FDiscovery/Debug] Build"
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "bmp",
            "cwd": "${workspaceRoot}",
            "executable": "./src/build/app/cardamom/debug/cardamom.elf",
            "name": "[Cardamom/FW]",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/misc/STM32F40x.svd",
            "device": "STM32F405",
            "BMPGDBSerialPort": "/dev/ttyBmpGdb",
            "targetId": 1,
            "preLaunchTask": "[FW/Cardamom/Debug] Build"
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "bmp",
            "cwd": "${workspaceRoot}",
            "executable": "./src/build/app/opendeck2/debug/opendeck2.elf",
            "name": "[OpenDeck/FW]",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/misc/STM32F40x.svd",
            "device": "STM32F405",
            "BMPGDBSerialPort": "/dev/ttyBmpGdb",
            "targetId": 1,
            "preLaunchTask": "[FW/OpenDeck2/Debug] Build"
        },
        {
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "bmp",
            "cwd": "${workspaceRoot}",
            "executable": "./src/build/app/rooibos/debug/rooibos.elf",
            "name": "[Rooibos/FW]",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/misc/STM32F40x.svd",
            "device": "STM32F405",
            "BMPGDBSerialPort": "/dev/ttyBmpGdb",
            "targetId": 1,
            "preLaunchTask": "[FW/Rooibos/Debug] Build"
        },
        {
            "name": "Flashgen",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/build/flashgen/opendeck2/debug/opendeck2.elf",
            "args": [],
            "stopAtEntry": true,
            "environment": [],
            "externalConsole": false,
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "[Flashgen] Build"
        }
    ]
}